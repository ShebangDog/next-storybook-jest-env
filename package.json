{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test:vrt": "run-all -s capture compare",
    "compare-no-story": "storycap --serverCmd \"start-storybook -p 6001\" http://localhost:6001",
    "compare": "reg-cli __screenshots__/actual __screenshots__/expected __screenshots__/diff -R ./report.html",
    "capture": "storycap --outDir __screenshots__/actual http://localhost:6006",
    "update-expected": "run-s update-expected:reg-cli",
    "update-expected:reg-cli": "reg-cli __screenshots__/actual __screenshots__/expected __screenshots__/diff  -U",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "screenshot": "storycap -C puppeteer --serverCmd \"start-storybook -p 6006\" http://localhost:6006 --serverTimeout 60000",
    "gen:ui": "hygen new ui",
    "gen:page": "hygen new page",
    "gen:model": "hygen new model",
    "gen:functional": "hygen new functional",
    "ls": "jq -r '.scripts|to_entries[]|((.key))' package.json"
  },
  "dependencies": {
    "@testing-library/react": "^13.3.0",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "recoil": "^0.7.4",
    "storycap": "^3.1.8",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/react": "^6.5.8",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/testing-react": "^1.3.0",
    "@swc/core": "^1.2.198",
    "@swc/helpers": "^0.4.2",
    "@swc/jest": "^0.2.21",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "^28.1.1",
    "@types/node": "17.0.41",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "^5.1.25",
    "babel-loader": "^8.2.5",
    "enquirer": "^2.3.6",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-storybook": "^0.5.12",
    "hygen": "^6.2.7",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "reg-cli": "^0.17.7",
    "reg-notify-github-plugin": "^0.12.1",
    "regenerator-runtime": "^0.13.9",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "4.7.3",
    "reg-suit": "^0.12.1"
  }
}
